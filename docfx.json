{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "azure",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "**/*..yml",
		  "azure/index.md"
        ]
      },
      {
        "files": [
          "azure/index.md"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/../",
        "exclude": []
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-services",
        "dest": "api/overview/azure"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "docs-ref-autogen",
        "dest": "."
      }
    ],
    "overwrite": "docs-ref-overwrite/*.md",
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [],
    "dest": "azure/sdk/node"
  }
}
